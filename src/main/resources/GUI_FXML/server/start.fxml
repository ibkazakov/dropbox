<AnchorPane prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="server.gui.contollers.StartController">

    <VBox layoutX="88.0" layoutY="40.0" prefHeight="78.0" prefWidth="324.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">

    <HBox maxWidth="Infinity" prefWidth="271.0">
